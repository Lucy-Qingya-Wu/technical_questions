import java.util.*;
public class StackWithMin<Integer> {


    // private class Node, value: data, min value under it including itself

    private static class StackNode<Integer>{

        private Integer value;
        private StackNode<Integer> minNode;
        private StackNode<Integer> next;

        public StackNode(Integer value){
            this.value=value;
            this.minNode=null;
            this.next=null;
        }

    }

    private StackNode<Integer> top;

    public void push(Integer v){
        if (top == null){
            top = new StackNode<Integer>(v);
            top.minNode=top;
        }else{
            Integer minNodeValue = top.minNode.value;

            if (v.intValue){
                minNodeValue = value;
            }


        }
    }

    public Integer pop(){

    }

    public Integer findMin(){

    }



}
